@model IEnumerable<site1.Models.Kullanıcılar>

@{
    ViewBag.Title = "KAYITLAR";
}

<h2>KAYITLAR</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.KULLANICIADI)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SIFRE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EMAİL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TARİH)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.KULLANICIADI)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SIFRE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EMAİL)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TARİH)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
